package cps161_FinalProject;
public abstract class FunkyToken {
    protected char token;
    protected boolean active; // Add the active field

    public FunkyToken(char token) {
        this.token = token;
        this.active = true; // Set active to true by default
    }

    public char getToken() {
        return token;
    }

    public boolean isActive() {
        return active;
    }

    public void setActive(boolean active) {
        this.active = active;
    }

    public abstract void move(FunkyBoard board);
}
